super + Return
	$TERM
super + m
	pactl set-source-mute @DEFAULT_SOURCE@ toggle
super + shift + r
    $TERM -e htop
super + shift + m
    pamixer -t # Toggle mute audio
super + r
    $TERM -e yazi
super + shift + w
    $TERM -e nmtui
super + shift + {1-9}
    bspc node -d '^{1-9}'
super + shift + {Down,Up}
    brightnessctl set {5-,+5}
super + w
    firefox
super + e
    chromium
super + {1-9}
    bspc desktop -f '^{1-9}'
super + {f,t,+ shift + f}
    bspc node -t {fullscreen,tiled,floating}
super + {h,j,k,l}
	bspc node -f {west,south,north,east}
super + shift + {h,j,k,l}
	bspc node -s {west,south,north,east}
super + {minus,equal}
    pamixer -{d,i} 5
super + {q,+ shift + q}
    bspc node -{c,k}
super + d
    rofi -show drun
super + shift + e
    rofi -modi emoji -show emoji
super + s
    _PATH_=$HOME/Pictures/Screenshots/pic-full-$(date '+%y%m%d-%H%M-%S').png && maim $_PATH_ && xclip -selection clipboard -t image/png $_PATH_
super + shift + s
    _PATH_=$HOME/Pictures/Screenshots/pic-sel-$(date '+%y%m%d-%H%M-%S').png && maim -s --noopengl $_PATH_ && xclip -selection clipboard -t image/png $_PATH_
super + p
    pavucontrol
super + shift + q
    systemctl suspend
super + bracketleft
    bspc node -z left -120 0 || bspc node -z right -120 0
super + bracketright
    bspc node -z left 120 0 || bspc node -z right 120 0
super + i
    sh $HOME/.dotfiles/scripts/switch_layout.sh
super + x
    nmcli device connect wlp3s0
